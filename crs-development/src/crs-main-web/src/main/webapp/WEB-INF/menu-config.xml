<?xml version="1.0" encoding="UTF-8" ?>
<MenuConfig>

    <Displayers>
        <Displayer name="Simple"
            type="net.sf.navigator.displayer.SimpleMenuDisplayer" />
        <Displayer name="CSS"
            type="net.sf.navigator.displayer.CSSListMenuDisplayer" />
        <Displayer name="Tabbed"
            type="net.sf.navigator.displayer.TabbedMenuDisplayer" />
        <Displayer name="DropDown"
            type="net.sf.navigator.displayer.DropDownMenuDisplayer" />
        <Displayer name="Cool"
            type="net.sf.navigator.displayer.CoolMenuDisplayer" />
    </Displayers>

    <Menus>
        <Menu name="mainMenu" title="">
            <Item name="ticketSection" title="Tickets">
                <Item name="tickets" title="Search" toolTip="Tickets Search"
                    image="images/menu-icons/emptyicon.png" page="/tickets-search.action?resetSearch=true" />
                <Item name="ticketHistory" title="History"
                    toolTip="Tickets History Search" image="images/menu-icons/emptyicon.png"
                    page="/ticketshistory-search.action?resetSearch=true" />
                <Item name="ticketReports" title="Reports" toolTip="Hostmaster Usage"
                    image="images/menu-icons/emptyicon.png" page="/reports-input.action?resetSearch=true" />
            </Item>
            <Item name="domainSection" title="Domains">
                <Item name="domains" title="Search" toolTip="Domains Search"
                    image="images/menu-icons/emptyicon.png" page="/domains-search.action?resetSearch=true" />
                <Item name="historicalDomains" title="History"
                    toolTip="Historical Domains Search" image="images/menu-icons/emptyicon.png"
                    page="/historical-domain-search.action?resetSearch=true" />
                <Item name="exportAuthCodes" title="Export Authcodes"
                    toolTip="Export Authcodes" image="images/menu-icons/emptyicon.png"
                    page="/domainExportAuthCodes-search.action?resetSearch=true" />
                <Item name="lockingServiceReport" title="Locking Service"
                    toolTip="Domains With Locking Service" image="images/menu-icons/emptyicon.png"
                    page="/lockingDomains-search.action?resetSearch=true" />
            </Item>
            <Item name="secondaryMarketSection" title="Registrant Transfer">
                <Item name="buyRequests" title="Buy Requests" toolTip="Buy Requests Search"
                      image="images/menu-icons/emptyicon.png" page="/buy-requests-search.action?resetSearch=true" />
                <Item name="buyRequestHistory" title="History" toolTip="Buy Request History"
                      image="images/menu-icons/emptyicon.png" page="/buy-request-history-search.action?resetSearch=true" />
                <Item name="sellRequests" title="Sell Requests" toolTip="Sell Requests Search"
                      image="images/menu-icons/emptyicon.png" page="/sell-requests-search.action?resetSearch=true" />
                <Item name="sellRequestHistory" title="History" toolTip="Sell Request History"
                      image="images/menu-icons/emptyicon.png" page="/sell-request-history-search.action?resetSearch=true" />
                <Item name="saleAuthcodes" title="Sale Authcodes" toolTip="Sale Authcodes Search"
                      image="images/menu-icons/emptyicon.png" page="/sale-authcodes-search.action?resetSearch=true" />
            </Item>
            <Item name="emailTemplateSection" title="Emails">
                <Item name="emailTemplates" title="View"
                    toolTip="Search email templates" image="images/menu-icons/emptyicon.png"
                    page="/emailtemplates-search.action" />
                <Item name="emailGroups" title="Groups" toolTip="View email groups"
                    image="images/menu-icons/emptyicon.png" page="/emailgroups-search.action" />
            </Item>
            <Item name="nicHandleSection" title="Nic Handles">
                <Item name="nicHandles" title="Search" toolTip="Nic Handles Search"
                    image="images/menu-icons/emptyicon.png" page="/nic-handles-search.action?resetSearch=true" />
                <Item name="createNicHandle" title="Create"
                    toolTip="Nic Handle Create" image="images/menu-icons/emptyicon.png"
                    page="/nic-handle-create-input.action" />
            </Item>
            <Item name="documentSection" title="Documents"
                toolTip="Displays new and assigned documents">
                <Item name="searchDocuments" title="Search"
                    toolTip="Search Documents" image="images/menu-icons/emptyicon.png"
                    page="/documents-assigned.action" parent_name="documents" />
                <Item name="newDocuments" title="New" toolTip="New Documents"
                    image="images/menu-icons/emptyicon.png" page="/documents-incoming.action"
                    parent_name="documents" />
            </Item>
            <Item name="accountSection" title="Accounts" toolTip="Accounts">
                <Item name="searchAccounts" title="Search" toolTip="Search Accounts"
                    image="images/menu-icons/emptyicon.png" page="/accounts-search.action?resetSearch=true" />
                <Item name="createAccounts" title="Create" toolTip="Create Account"
                    image="images/menu-icons/emptyicon.png" page="/account-create-input.action" />
            </Item>
            <Item name="DSMSection" title="DSM">
                <Item name="forceEvent" title="Force Event"
                    toolTip="Force DSM Event" image="images/menu-icons/emptyicon.png"
                    page="/forceDSMevent-input.action" />
                <Item name="forceState" title="Force State"
                    toolTip="Force DSM State" image="images/menu-icons/emptyicon.png"
                    page="/forceDSMstate-input.action" />
            </Item>
            <Item name="vatSection" title="Vat">
                <Item name="viewVat" title="View" toolTip="View vat rate"
                    image="images/menu-icons/emptyicon.png" page="/vatView-input.action" />
                <Item name="createVat" title="Create" toolTip="Create vat rate"
                    image="images/menu-icons/emptyicon.png" page="/vatCreate-input.action" />
            </Item>
            <Item name="taskSection" title="Task">
                <Item name="taskView" title="View" toolTip="View task"
                    image="images/menu-icons/emptyicon.png" page="/task-view-input.action" />
                <Item name="taskCreate" title="Create" toolTip="Create task"
                    image="images/menu-icons/emptyicon.png" page="/task-create-input.action" />
                <Item name="jobView" title="Job" toolTip="View Job"
                    image="images/menu-icons/emptyicon.png" page="/job-view-input.action?resetSearch=true" />
                <Item name="jobHist" title="Job Hist" toolTip="View Job Hitory"
                    image="images/menu-icons/emptyicon.png" page="/job-hist-view-input.action?resetSearch=true" />
            </Item>
            <Item name="pricingSection" title="Product Pricing">
                <Item name="viewPrice" title="View" toolTip="View product price"
                    image="images/menu-icons/emptyicon.png" page="/priceView-input.action" />
                <Item name="createPrice" title="Create"
                    toolTip="Create product price" image="images/menu-icons/emptyicon.png"
                    page="/priceCreate-input.action" />
            </Item>
            <Item name="crsConfigurationSection" title="CRS configuration">
                <Item name="crsConfiguration" title="View"
                    toolTip="View application configuration" image="images/menu-icons/emptyicon.png"
                    page="/configView-input.action" />
            </Item>
            <Item name="bulkTransferSection" title="Bulk transfer">
                <Item name="createBulkTransfer" title="Create"
                    toolTip="New bulk transfer" image="images/menu-icons/emptyicon.png"
                    page="/bulkTransferCreate-input.action" />
                <Item name="viewBulkTransfer" title="View"
                    toolTip="View bulk transfers" image="images/menu-icons/emptyicon.png"
                    page="/bulkTransferList-input.action" />
            </Item>
            <Item name="reportSection" title="Accounts Report">
                <Item name="depositReport" title="Deposit"
                    toolTip="View deposit actual/available balance"
                    image="images/menu-icons/emptyicon.png"
                    page="/depositAccount-input.action?resetSearch=true" />
                <Item name="doaReport" title="DOA" toolTip="View DOA"
                    image="images/menu-icons/emptyicon.png" page="/viewDOA-input.action?resetSearch=true" />
                <Item name="invoiceReport" title="Invoices"
                    toolTip="View Invoices" image="images/menu-icons/emptyicon.png"
                    page="/viewInvoices-input.action?resetSearch=true" />
                <Item name="extendedReservationReport" title="Revenue Assurance"
                    toolTip="View Revenue Assurance" image="images/menu-icons/emptyicon.png"
                    page="/viewExtendedReservations-input.action?resetSearch=true" />
                <Item name="extendedReservationReport" title="Receipts: Bank Reconciliation"
                    toolTip="View Receipts: Bank Reconciliation" image="images/menu-icons/emptyicon.png"
                    page="/bankReceipts-input.action?resetSearch=true" />
                <Item name="nrpReport" title="NRP" toolTip="View NRP Domains"
                    image="images/menu-icons/emptyicon.png" page="/nrpDomains-input.action?resetSearch=true" />
                <Item name="charityReport" title="Charity"
                    toolTip="View Charity Domains" image="images/menu-icons/emptyicon.png"
                    page="/charityDomains-input.action?resetSearch=true" />
                <Item name="totalDomainsReport" title="TotalDomains"
                    toolTip="View Total Domains per Registrar Report"
                    image="images/menu-icons/emptyicon.png"
                    page="/totalDomainsReport-input.action?resetSearch=true" />
                <Item name="totalDomainsPerDateReport" title="Registrations"
                    toolTip="View Registrations Per Month/Year for Registrar/All Registrars"
                    image="images/menu-icons/emptyicon.png"
                    page="/totalDomainsPerDateReport-input.action?resetSearch=true" />
                <Item name="perClassReport" title="Per Class"
                    toolTip="Domains Per Class/category" image="images/menu-icons/emptyicon.png"
                    page="/domainsPerClassReport-input.action?resetSearch=true" />
                <Item name="deletedDomainsReport" title="Deleted"
                    toolTip="Deleted Domains" image="images/menu-icons/emptyicon.png"
                    page="/deleted-input.action?resetSearch=true" />
                <Item name="autorenewReport" title="Autorenews"
                    toolTip="Autorenews" image="images/menu-icons/emptyicon.png"
                    page="/autorenews-input.action?resetSearch=true" />
                <Item name="prolongReport" title="Prolong Locking Service"
                    toolTip="Prolong Locking Service By Rolling Renewal Date"
                    image="images/menu-icons/emptyicon.png"
                    page="/domainsRollLockingService-search.action?resetSearch=true" />
            </Item>
        </Menu>
    </Menus>

</MenuConfig>
