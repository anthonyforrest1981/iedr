<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
        PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "sql-map-2.dtd">

<sqlMap namespace="top-level-domain">

    <select id="findTopLevelDomains" resultMap="common.normalizedString">
        SELECT
        Name as
        domainName
        FROM
        TopLevelDomain
    </select>

</sqlMap>